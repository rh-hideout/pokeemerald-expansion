script LittlerootTown_EventScript_NewLittlerootTownSign {
    lockall
	msgbox("Testing!")
	special(NameRival)
	waitstate
}

script LittlerootTown_EventScript_GymChampionCheck {
    lockall
    faceplayer

    if (!flag(FLAG_UNUSED_0x020)) {
        call(GymVerifyIntro)
    } elif (flag(FLAG_UNUSED_0x020)) {
		goto(Verification)
	}
}


script ChampionVerified {
    msgbox(format("DING DING DING! Hall of Fame Status...Confirmed!"))
    msgbox(format("CharonChaos: You're the Hoenn Champion. Congratulations!"))
    msgbox(format("CharonChaos: Maxie has requested me to send you his blessings. Enjoy the new Littleroot Town for saving our lives!"))
    closemessage
    lockall
    applymovement(9, MagmaGruntLeave)
    waitmovement(0)
    removeobject(9)
    msgbox(format("Congratulations {PLAYER}! You now have access to New Littleroot Town!"))
    releaseall
    end
  }

movement MagmaGruntLeave {
    walk_right * 3
    walk_up * 5
}

script ChampionNotVerified {
    msgbox(format("Error: Current name {PLAYER} is not a Hoenn Champion!"))
    msgbox(format("CharonChaos: Bummer... Looks like you got ways to go."))
    msgbox(format("CharonChaos: Come back when you become the Hoenn Champion."))
    closemessage
    end
}

script GymVerifyIntro {
    msgbox(format("CharonChaos: OKAY! OKAY! I'll have it done before the new champion enters the hall of fame."))
    msgbox(format("CharonChaos: Oh my bad! I was talking to my boss. Hello, I am CharonChaos!"))
    msgbox(format("CharonChaos: What's your name?"))
    msgbox(format("CharonChaos: ... {PLAYER}? Cool!"))
    msgbox(format("CharonChaos: You might be wondering why I am guarding this place. Well, it is part of Littleroot Town. In fact, It is New Littleroot Town."))
    msgbox(format("CharonChaos: I was contracted by Team Magma..."))
    msgbox(format("CharonChaos: No I am not a bad guy. I'll explain."))
    msgbox(format("CharonChaos: I was a former Hoenn Champion a while back and I recently retired."))
    msgbox(format("CharonChaos: Anyways..."))
    msgbox(format("CharonChaos: In order for you to pass {PLAYER}, you need to become the Hoenn region's champion. Otherwise, I cannot let you in New Littleroot Town."))
	setflag(FLAG_UNUSED_0x020)
	goto(Verification)
    closemessage
    end
}

// Always attempt verification after the intro
script Verification {
    msgbox(format("CharonChaos: Let me attempt to verify your name {PLAYER} to the Hoenn region Hall of Fame database..."))
	if (flag(FLAG_IS_CHAMPION) == true) {
	goto(ChampionVerified)
	} elif (flag(FLAG_IS_CHAMPION) == false) {
		goto(ChampionNotVerified)
	}	
  }
