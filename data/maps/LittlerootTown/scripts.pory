script NicknameBoxMon{
	msgbox("Would you like to give your starter a nickname?", MSGBOX_YESNO)
	waitmessage
    hidemonpic
    if(var(VAR_RESULT) == YES){
        call(Common_EventScript_NameReceivedBoxMon)
		closemessage
		release
    }
    msgbox("Transferred to PC")
}

script NicknamePartyMon{
    msgbox("Would you like to give your starter\na nickname?", MSGBOX_YESNO)
	waitmessage
    if(var(VAR_RESULT) == YES){
        call(Common_EventScript_GetGiftMonPartySlot)
        call(Common_EventScript_NameReceivedPartyMon)
		setflag(FLAG_SYS_POKEMON_GET)
    	hidemonpic
		closemessage
		release
		end		 
		} elif (var(VAR_RESULT) == NO) {
        call(Common_EventScript_GetGiftMonPartySlot)
		setflag(FLAG_SYS_POKEMON_GET)
		hidemonpic
		closemessage
		release
		end
    }
	}
script StarterKit {
	msgbox(format("{PLAYER} looks further into the bag."))
	giveitem(ITEM_POKE_BALL, 10)
	msgbox(format("{PLAYER} obtained 10 Poké Balls."))	
	giveitem(ITEM_POTION, 5)
	msgbox(format("{PLAYER} obtained 5 Potions."))
	release
	end
}
